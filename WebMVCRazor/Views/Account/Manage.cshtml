@{
    ViewBag.Title = "Manage Account";
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

<p class="text-success">@ViewBag.StatusMessage</p>
<div class="row">
    <div class="col-md-12">
        <h3>@ViewBag.Title</h3>
        <hr/>       
        @(ViewBag.HasLocalPassword ? Html.Partial("_ChangePasswordPartial") : Html.Partial("_SetPasswordPartial"))

